<div class="col">
  <div class="card h-100 mb-3">
    <img src="https://avatars.githubusercontent.com/u/<%= $_a %>" alt="Image: avatar" class="card-img-top" title="<%= $_n %>">
    <div class="card-body">
      <h5 class="card-title"><%= $_n %></h5>
      <p class="card-text small"><%= ${_b//_/ } %></p>
      <div class="social">
        <a href="https://github.com/<%= $_c %>/" target="_blank"><img src="/a/github.svg" alt="Image: GitHub loog" class="img-fluid" title="@<%= $_c %>"></a>
        <a href="https://t.me/<%= $_d %>" target="_blank"><img src="/a/telegram.svg" alt="Image: Telegram logo" class="img-fluid" title="@<%= $_d %>"></a>
      </div>
    </div>
  </div>
</div>
